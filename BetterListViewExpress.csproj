<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <AssemblyName>BetterListViewExpress</AssemblyName>
    <TargetFramework>net6.0-windows</TargetFramework>
    <GenerateAssemblyInfo>False</GenerateAssemblyInfo>
    <UseWindowsForms>True</UseWindowsForms>
  </PropertyGroup>
  <PropertyGroup>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup>
    <RootNamespace />
  </PropertyGroup>
  <ItemGroup>
    <None Remove="BetterListView.Resources.BetterListView.bmp" />
    <None Remove="BetterListView.Resources.TrialHeader.png" />
    <EmbeddedResource Include="BetterListView.Resources.BetterListView.bmp" />
    <EmbeddedResource Include="BetterListView.Resources.TrialHeader.png" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Design" />
    <Reference Include="System.Xml" />
  </ItemGroup>
</Project>